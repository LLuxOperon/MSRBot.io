{
  "name": "doc-registry",
  "version": "1.0.0-beta.1",
  "description": "Doc Registry",
  "scripts": {
    "build": "node src/main/scripts/build.js",
    "build-msi": "node src/main/scripts/buildMasterSuiteIndex.js --in src/main/data/documents.json --out src/main/reports/masterSuiteIndex.json",
    "build-mri": "node src/main/scripts/buildMasterReferenceIndex.js",
    "extract": "node src/main/scripts/extractDocs.js",
    "normalize-url": "node src/main/scripts/url.normalize.js --apply",
    "validate": "node src/main/scripts/validate.js",
    "validate-url": "node src/main/scripts/url.validate.js",
    "canonicalize": "node src/main/scripts/canonicalize.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveLLamb/doc-registry.git"
  },
  "author": "Steve LLamb",
  "license": "CC-BY-4.0",
  "devDependencies": {
    "ajv": "^6.12.6",
    "handlebars": "^4.7.7"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "bootstrap-icons": "^1.9.1",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.13",
    "json-2-csv": "^3.17.1",
    "json-source-map": "^0.6.1",
    "json-stable-stringify": "^1.0.1",
    "node": "^22.18.0",
    "stringify": "^5.2.0",
    "yargs": "^16.2.0"
  }
}
