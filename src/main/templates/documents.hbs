<!DOCTYPE html>
<html lang="en">

  {{>header}}

    <!-- Main content starts - do not edit above this line -->
    <table class="table table-small table-hover">
      <thead class="thead-light">
        <tr>
          <th scope="col" width="4em">Label</th>
          <th scope="col">Publisher</th>
          <th scope="col">Type</th>
          <th scope="col">Title/Details</th>
          <th scope="col"></th>    
        </tr>
      </thead>
      <tbody id="searchTable">
        {{#data}}
        <tr id="{{../idType}}-{{docId}}">
          <td class="label">
            <a class="anchor" id="{{docId}}" href="#{{docId}}"></a><code>{{label}}</code>
            {{#inprocess}}
              <br><span class="mx-3 badge badge-success">In Process</span>    
            {{/inprocess}}
            {{#withdrawn}}
              <br><span class="mx-3 badge badge-danger">Withdrawn</span>  
            {{/withdrawn}}
            {{#superseded}}
              <br><span class="mx-3 badge badge-warning">Superseded</span> 
            {{/superseded}}
            {{#stablized}}
              <br><span class="mx-3 badge badge-primary">Stablized</span>    
            {{/stablized}}
            {{#reaffirmed}}
              <br><span class="mx-3 badge badge-info">Reaffirmed</span>   
            {{/reaffirmed}}
            {{#amended}}
              <br><span class="mx-3 badge badge-secondary">Amended</span>    
            {{/amended}}
          </td>
          <td class="publisher">
            {{publisher}}
            {{#if group}}
              <br>
              [{{group}}]
            {{/if}}
          </td>
          <td>{{type}}</td>
          <td>
            <span class="title">
              {{#if url}}
                <a class="{{../idType}}-link" target="_blank" href="{{url}}">
              {{/if}}
              {{title}}
              {{#if url}}
                </a>
              {{/if}}
            </span>
            <div class="container bg-light">
              <div class="row">
                <div class="col-sm">
                  {{#if status}}
                  <span class="font-weight-bold">Current Status: <em>{{status}}</em></span>
                  <br>
                  {{/if}}
                  {{#if publicationDate}}
                  Publication Date: {{publicationDate}}
                  <br>
                  {{/if}}
                  {{#if withdrawnDate}}
                  Withdrawn Date: {{withdrawnDate}}
                  <br>
                  {{/if}}
                  {{#if stablizedDate}}
                  Stablized Date: {{stablizedDate}}
                  <br>
                  {{/if}}
                  {{#if reaffirmDate}}
                  Reaffirmed Date: {{reaffirmDate}}
                  <br>
                  {{/if}}
                  {{#if supersededDate}}
                  Superseded Date: {{supersededDate}}
                  <br>
                  {{/if}}
                  {{#if amendedDate}}
                  Amended Date: {{amendedDate}}
                  <br>
                  {{/if}}
                  {{#if details}}
                  Details: {{details}}
                  <br>
                  {{/if}}
                  {{#if github}}
                  GitHub: <a class="{{../idType}}-link" target="_blank" href="{{github}}">{{github}}</a>
                  <br>
                  {{/if}}
                  {{#if draftInfo}}
                  <div class="col-sm">
                    <span class="font-weight-bold"><u>Draft Info</u><br></span>
                    Group: 
                    {{#if draftInfo/groupLink}}
                    <a class="{{../idType}}-link" target="_blank" href="{{draftInfo/groupLink}}">
                    {{/if}}
                    {{draftInfo/group}}
                    {{#if draftInfo/groupLink}}
                    </a>
                    {{/if}}
                    {{#if draftInfo/recommendation}}
                    <br>
                    Recommendation: {{draftInfo/recommendation}}
                    {{/if}}
                    {{#if draftInfo/approved}}
                    <br>
                    TC Approved: Yes
                    {{/if}}
                    {{#if draftInfo/notes}}
                    <br>
                    Notes: {{draftInfo/notes}}
                    {{/if}}
                    {{#if draftInfo/assigned}}
                    <br>
                    Assigned to: {{draftInfo/assigned}}
                    {{/if}}
                    {{#if draftInfo/actionItems}}
                    <br>
                    <a class="{{../idType}}-link" target="_blank" href="{{draftInfo/actionItems}}">Action Items</a>
                    {{/if}}
                    {{#if draftInfo/projectLink}}
                    <br>
                    <a class="{{../idType}}-link" target="_blank" href="{{draftInfo/projectLink}}">Project Link</a>
                    {{/if}}
                    {{#if draftInfo/updateDependency}}
                    <br><span>Dependent on updates to:</span>
                    {{/if}}
                      {{#draftInfo/updateDependency}}
                        <br><a class="{{../../idType}}-link" href="#{{this}}">{{this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span>
                      {{/draftInfo/updateDependency}}
                  </div>
                {{/if}}
                </div>
                {{#if references}}
                  <div class="col-sm">
                    {{#if references/normative}}
                      <span class="font-weight-bold">Normative References:<br></span>
                    {{/if}}
                      {{#references/normative}}
                        <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br> 
                      {{/references/normative}}
                    {{#if references/bibliographic}}
                      <span class="font-weight-bold">Bibliographic References:<br></span>
                    {{/if}}
                      {{#references/bibliographic}}
                        <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br>
                      {{/references/bibliographic}}
                  </div>
                {{/if}}
              </div>
            </div>
          </td>
          <td>
            {{#if amendedBy}}
                <span class="font-weight-bold">Amended by:<br></span>
              {{/if}}
              {{#amendedBy}}
                <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br> 
            {{/amendedBy}}
            {{#if supersededBy}}
                <span class="font-weight-bold">Superseded by:<br></span>
              {{/if}}
              {{#supersededBy}}
                <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br> 
              {{/supersededBy}}
          </td>
        </tr>
        {{/data}}
      </tbody>
    </table>
    <!-- Main content ends - do not edit below this line -->
{{>footer}}

</html>