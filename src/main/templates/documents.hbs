<!DOCTYPE html>
<html lang="en">

  {{>header}}

    <!-- Main content starts - do not edit above this line -->
    <table id="sorttable" class="table table-small table-hover w-auto display">
      <thead class="thead-light">
        <tr>
          <th scope="col" width="10em">Label</th>
          <th scope="col" width="8em">Publisher</th>
          <th scope="col" width="8em">Type</th>
          <th scope="col" width="10em">Usage</th>
          <th scope="col" width="15em">Status</th>
          <th scope="col-lg-8">Details</th>    
        </tr>
      </thead>
      <tbody id="searchTable">
        {{#data}}
        <tr id="{{../idType}}-{{docId}}">
          <td class="doclabel">
            <a class="anchor" id="{{docId}}" href="#{{docId}}"></a><code>{{label}}</code>
          </td>
          <td class="publisher">
            {{publisher}}{{#if group}} [{{group}}]{{/if}}
          </td>
          <td>
            {{type}}
          </td>
          <td>
            <div class="container-fluid">
              {{#usage}}
              <span class="small"><code>[{{this}}]</code></span><br>
              {{/usage}}
            </div>
          </td>
          <td>
            <div class="container-fluid">
              {{#unknown}}
                <span class="label badge badge-danger">[ Unknown ]</span><br> 
              {{/unknown}}
              {{#withdrawn}}
                <span class="label badge badge-danger">[ Withdrawn ]</span><br> 
              {{/withdrawn}}
              {{#superseded}}
                <span class="label badge badge-warning">[ Superseded ]</span><br>
              {{/superseded}}
              {{#if supersededBy}}
                <div class="border-left p-1">
                  <span class="font-weight-bold small">Superseded by:</span><br>
                {{/if}}
                {{#supersededBy}}
                  <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{getLabel this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br> 
                {{/supersededBy}}
                {{#if supersededBy}}
                </div>
              {{/if}}
              {{#draft}}
                <span class="label badge badge-warning">[ Draft ]</span><br> 
              {{/draft}}
              {{#active}}
                <span class="label badge badge-success">[ Active ]</span><br>  
              {{/active}}
              {{#amended}}
                <span class="label badge badge-secondary">[ Amended ]</span><br>
              {{/amended}}
              {{#if amendedBy}}
                <div class="border-left p-1">
                  <span class="font-weight-bold small">Amended by:</span><br>
                {{/if}}
                {{#amendedBy}}
                  <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{getLabel this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br> 
                {{/amendedBy}}
                {{#if amendedBy}}
                </div>
              {{/if}}
              {{#reaffirmed}}
                <span class="label badge badge-info">[ Reaffirmed ]</span><br>
              {{/reaffirmed}}
              {{#stablized}}
                <span class="label badge badge-primary">[ Stablized ]</span>  
              {{/stablized}}   
            </div>      
          </td>
          <td>
            <div class="container-fluid">
              <div class="row">
                <span class="title">
                {{#if url}}
                  <a class="{{../idType}}-link" target="_blank" href="{{url}}">
                {{/if}}
                {{title}}
                {{#if url}}
                  </a>
                {{/if}}
                </span>
              </div>
              <div class="row p-2">
                <div class="col p-2">
                  <p>
                    <span class="font-weight-bold small">Current Status:</span> <em>{{status}}</em>{{#if statusNote}}<span class="small"><em> ({{statusNote}})</em></span>{{/if}}
                  {{#if details}}
                  <br>
                    <span class="font-weight-bold small">Details:</span> <em>{{details}}</em>
                  {{/if}}
                  </p>
                  <p>
                    {{#if publicationDate}}
                    <span class="font-weight-bold small">Publication Date:</span> {{publicationDate}}
                    <br>
                    {{/if}}
                    {{#if withdrawnDate}}
                    <span class="font-weight-bold small">Withdrawn Date:</span> {{withdrawnDate}}
                    <br>
                    {{/if}}
                    {{#if stablizedDate}}
                    <span class="font-weight-bold small">Stablized Date:</span> {{stablizedDate}}
                    <br>
                    {{/if}}
                    {{#if reaffirmDate}}
                    <span class="font-weight-bold small">Reaffirmed Date:</span> {{reaffirmDate}}
                    <br>
                    {{/if}}
                    {{#if supersededDate}}
                    <span class="font-weight-bold small">Superseded Date:</span> {{supersededDate}}
                    <br>
                    {{/if}}
                    {{#if amendedDate}}
                    <span class="font-weight-bold small">Amended Date:</span> {{amendedDate}}
                    <br>
                    {{/if}}
                    {{#if github}}
                    <span class="font-weight-bold small">GitHub:</span> <a class="{{../idType}}-link" target="_blank" href="{{github}}">{{github}}</a>
                    <br>
                    {{/if}}
                  </p>
                  {{#if workInfo}}
                    <div class="border-left p-2">
                        <span class="font-weight-bold"><u>Work Info</u><br></span>
                        <span class="font-weight-bold small">Assigned Group: </span>
                        {{#if workInfo/groupLink}}
                        <a class="{{../idType}}-link" target="_blank" href="{{workInfo/groupLink}}">
                        {{/if}}
                        {{workInfo/group}}
                        {{#if workInfo/groupLink}}
                        </a>
                        {{/if}}
                        {{#if workInfo/actionItems}} | <a class="{{../idType}}-link" target="_blank" href="{{workInfo/actionItems}}">Action Items</a>{{/if}}
                        {{#if workInfo/projectLink}} | <a class="{{../idType}}-link" target="_blank" href="{{workInfo/projectLink}}">Project Link</a>{{/if}}
                        {{#if workInfo/reviewNeeded}}
                        <br>
                        <span class="font-weight-bold small">Review Needed:</span> Yes
                        {{/if}}
                        {{#if workInfo/reviewPeriod}}
                        <br>
                        <span class="font-weight-bold small">Review Period:</span> {{workInfo/reviewPeriod}}
                        {{/if}}
                        {{#if workInfo/reviewDate}}
                        <br>
                        <span class="font-weight-bold small">Review Date:</span> {{workInfo/reviewDate}}
                        {{/if}}
                        {{#if workInfo/recommendation}}
                        <br>
                        <span class="font-weight-bold small">Recommendation:</span> {{workInfo/recommendation}}
                        {{/if}}
                        {{#if workInfo/approved}}
                        <br>
                        <span class="font-weight-bold small">TC Approved:</span> Yes
                        {{/if}}
                        {{#if workInfo/assigned}}
                        <br>
                        <span class="font-weight-bold small">Assigned to:</span> {{workInfo/assigned}}
                        {{/if}}
                        {{#if workInfo/updateDependency}}
                        <br><span class="font-weight-bold small">Dependent on updates to:</span>
                        {{/if}}
                        {{#workInfo/updateDependency}}
                        <br><a class="{{../../idType}}-link" href="#{{this}}">{{this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span>
                        {{/workInfo/updateDependency}}
                        {{#if workInfo/notes}}
                        <br><br>
                        <span class="font-weight-bold small">Notes:</span> <em>{{workInfo/notes}}</em>
                        {{/if}}
                        {{#if workInfo/otherWork}}
                        <br>
                        <span class="font-weight-bold small">Other Requested Work:</span> <em>{{workInfo/otherWork}}</em>
                        {{/if}}
                    </div>
                  {{/if}}
                  {{#if xmlNamespace}}
                  <div class="p-2">
                      <span class="font-weight-bold small">XML Namespace(s) Defined:</span>
                      {{#xmlNamespace}}
                      <br><a target="_blank" href="{{this}}">{{this}}</a>
                      {{/xmlNamespace}}
                  </div>
                  {{/if}}
                  {{#if relatedDocs}}
                  <div class="p-2">
                      <span class="font-weight-bold small">Related Document(s):</span>
                      {{#relatedDocs}}
                      <br><a class="{{../idType}}-link clear-filter" href="#{{this}}">{{getLabel this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span> 
                      {{/relatedDocs}}
                  </div>
                  {{/if}}
                </div>

                {{#if references/normative}}
                <div class="col-sm-3">
                  <p><span class="font-weight-bold small">Normative Reference(s):<br></span>
                  {{#references/normative}}
                    <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{getLabel this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br> 
                  {{/references/normative}}
                  </p>
                </div>
                {{/if}}

                {{#if references/bibliographic}}
                <div class="col-sm-3">
                  <p><span class="font-weight-bold small">Bibliographic Reference(s):<br></span>
                  {{#references/bibliographic}}
                    <a class="{{../../idType}}-link clear-filter" href="#{{this}}">{{getLabel this}}</a> <span class="small"><em>[{{getStatus this}}]</em></span><br>
                  {{/references/bibliographic}}
                  </p>
                </div>
                {{/if}}

              </div>
            </div>
          </td>
        </tr>
        {{/data}}
      </tbody>
    </table>
    <!-- Main content ends - do not edit below this line -->
{{>footer}}

</html>