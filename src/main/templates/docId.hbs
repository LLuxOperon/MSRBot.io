{{> header}}
<!-- Main content starts - do not edit above this line -->

<main class="container my-4">
  <header class="mb-3">
    <div class="d-flex align-items-center flex-wrap gap-2 mb-1">
      {{#if (publisherLink publisher)}}
        <a href="{{publisherLink publisher}}" target="_blank" rel="noopener">{{publisherLogo publisher}}</a>
      {{else}}
        {{publisherLogo publisher}}
      {{/if}}
      <code class="me-1 fs-5">{{docLabel}}</code>
      <div class="d-inline-flex flex-wrap gap-1">
        {{#if status.unknown}}   <span class="label badge text-bg-danger">[UNKNOWN]</span>{{/if}}
        {{#if status.withdrawn}} <span class="label badge text-bg-danger">[WITHDRAWN]</span>{{/if}}
        {{#if status.superseded}}<span class="label badge text-bg-warning">[SUPERSEDED]</span>{{/if}}
        {{#if status.draft}}     <span class="label badge text-bg-warning">[DRAFT]</span>{{/if}}
        {{#if status.publicCd}}  <span class="label badge text-bg-success">[PUBLIC CD]</span>{{/if}}
        {{#if status.active}}    <span class="label badge text-bg-success">[ACTIVE]</span>{{/if}}
        {{#if status.versionless}}<span class="label badge bg-success-subtle text-info-emphasis">[VERSIONLESS]</span>{{/if}}
        {{#if status.amended}}   <span class="label badge text-bg-secondary">[AMENDED]</span>{{/if}}
        {{#if status.reaffirmed}}<span class="label badge text-bg-info">[REAFFIRMED]</span>{{/if}}
        {{#if status.stabilized}}<span class="label badge text-bg-primary">[STABILIZED]</span>{{/if}}
      </div>
    </div>

    <h1 class="h4 mb-1">
      {{#if href}}
        <a class="text-decoration-none" href="{{href}}" target="_blank" rel="noopener">{{docTitle}}</a>
      {{else}}
        {{docTitle}}
      {{/if}}
    </h1>
  </header>

  <section class="row g-3">
    <div class="col-lg-7">
      <div class="card">
        <div class="card-header pb-0">
          <h2 class="h5 card-title">Metadata</h2>
        </div>
        <div class="card-body">
          <dl class="row small mb-0">
            {{#if status.statusNote}}<dt class="col-sm-4">Status Note</dt><dd class="col-sm-8"><em>{{status.statusNote}}</em></dd>{{/if}}
            {{#if publisher}}<dt class="col-sm-4">Publisher</dt><dd class="col-sm-8"><a class="clear-filter" href="../?f.publisher={{publisher}}">{{publisher}}</a></dd>{{/if}}
            {{#if docType}}<dt class="col-sm-4">Doc Type</dt><dd class="col-sm-8">{{docType}}{{#if docTypeAbr}} ({{docTypeAbr}}){{/if}}</dd>{{/if}}
            {{#if publicationDate}}<dt class="col-sm-4">Publication Date</dt><dd class="col-sm-8">{{publicationDate}}</dd>{{/if}}
            {{#if status.amendedDate}}<dt class="col-sm-4">Amended Date</dt><dd class="col-sm-8">{{status.amendedDate}}</dd>{{/if}}
            {{#if status.reaffirmDate}}<dt class="col-sm-4">Reaffirm Date</dt><dd class="col-sm-8">{{status.reaffirmDate}}</dd>{{/if}}
            {{#if status.stabilizedDate}}<dt class="col-sm-4">Stabilized Date</dt><dd class="col-sm-8">{{status.stabilizedDate}}</dd>{{/if}}
            {{#if status.supersededDate}}<dt class="col-sm-4">Superseded Date</dt><dd class="col-sm-8">{{status.supersededDate}}</dd>{{/if}}
            {{#if status.withdrawnDate}}<dt class="col-sm-4">Withdrawn Date</dt><dd class="col-sm-8">{{status.withdrawnDate}}</dd>{{/if}}
            {{#if doi}}<dt class="col-sm-4">DOI</dt><dd class="col-sm-8"><a href="https://doi.org/{{doi}}" target="_blank" rel="noopener"><code>{{doi}}</code></a></dd>{{/if}}
            {{#if href}}<dt class="col-sm-4">Link</dt><dd class="col-sm-8"><a href="{{href}}" target="_blank" rel="noopener" class="text-break d-inline-block" style="max-width:100%">{{href}}</a></dd>{{/if}}
            {{#if repo}}<dt class="col-sm-4">Repo</dt><dd class="col-sm-8"><a href="{{repo}}" target="_blank" rel="noopener" class="text-break d-inline-block" style="max-width:100%">{{repo}}</a></dd>{{/if}}
            {{#if authors}}<dt class="col-sm-4">Author(s)</dt><dd class="col-sm-8">{{#authors}}{{this}}{{#unless @last}}, {{/unless}}{{/authors}}</dd>{{/if}}
            {{#if group}}<dt class="col-sm-4">Group</dt><dd class="col-sm-8">{{#with (groupIdLookup dataGroups group)}}{{#if groupId}}<a href="/groups/{{../../htmlLink}}?search.search={{groupId}}">{{/if}}{{groupOrg}} {{groupName}} {{groupDesc}}{{#if groupId}}</a>{{/if}}{{/with}}</dd>{{/if}}
            {{#with (docProjLookup @root.docProjs docId)}}{{#if projectId}}<dt class="col-sm-4">Current Work</dt>
            <dd class="col-sm-8">
              {{#ifnoteq projectStatus "Complete"}}<span class="label badge text-bg-warning text-uppercase">{{/ifnoteq}}{{#ifeq projectStatus "Complete"}}<span class="label badge text-bg-light text-uppercase">{{/ifeq}}{{workType}} - {{projectStatus}}</span>
              <br>{{#ifnoteq projectStatus "Complete"}}<span class="small">Under Development: {{/ifnoteq}}<a href="/projects/{{../../htmlLink}}#{{projectId}}">Project - {{projectId}}</a> {{#if newDoc}} - New Doc: <a class="{{../../idType}}-link clear-filter" href="../{{newDoc}}">{{getLabel newDoc}}</a> [{{getStatus newDoc}}]</span> {{{getstatusButton newDoc "10"}}} {{#with (docProjLookup @root.docProjs newDoc)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}{{/if}}</span>
            </dd>{{/if}}{{/with}}
            {{#if keywords}}<dt class="col-sm-4">Keywords(s)</dt><dd class="col-sm-8">{{#keywords}}<a class="clear-filter" href="../?f.keywords={{this}}">{{this}}</a>{{#unless @last}}, {{/unless}}{{/keywords}}</dd>{{/if}}
            
            {{#if details}}<dt class="col-sm-4">Details</dt><dd class="col-sm-8"><em>{{details}}</em></dd>{{/if}}
          </dl>
        </div>
      </div>
      
      {{#if status/withdrawnNotice}}
      <div class="row small m-2">
        <span class="label text-center px-3 label-warn text-bg-warning text-uppercase">Withdrawl notice: <a href="{{status/withdrawnNotice}}">{{status/withdrawnNotice}}</a></span>
      </div>
      {{/if}}
      
      
    {{#if docBase}}
    <div class="p-2 text-muted text-center">
      <span class="fst-italic">
        {{#if status.latestVersion}}{{#unless status/withdrawn}}Latest version of {{docBaseLabel}}{{/unless}}{{else}}Newer version available!{{/if}}
      </span>
    </div>

    {{#if (len docSuite)}}
    <div class="card mt-2">
      <div class="card-body">
        <h2 class="h6 mb-2">Document Suite — {{docBaseLabel}}</h2>
        <ul class="list-group list-group-flush small">
          {{#each docSuite}}
          <li class="list-group-item d-flex align-items-center justify-content-between flex-wrap {{#ifeq ../docId docId}} bg-light{{/ifeq}}">
            <div class="me-2">
              {{#ifeq ../docId docId}}<strong>▶</strong>{{/ifeq}}
              <a class="clear-filter" href="../{{docId}}/">{{docLabel}}</a>
              {{#if publicationDate}} <span class="text-muted">({{publicationDate}})</span>{{/if}}
            </div>
            <div class="d-inline-flex flex-wrap gap-1">
              {{#ifeq ../docId docId}}   <span class="badge badge-dsuite text-bg-info">THIS DOC</span>{{/ifeq}}
              {{#if status.unknown}}     <span class="badge badge-dsuite text-bg-danger">[UNKNOWN]</span>{{/if}}
              {{#if status.withdrawn}}   <span class="badge badge-dsuite text-bg-danger">[WITHDRAWN]</span>{{/if}}
              {{#if status.superseded}}  <span class="badge badge-dsuite text-bg-warning">[SUPERSEDED]</span>{{/if}}
              {{#if status.draft}}       <span class="badge badge-dsuite text-bg-warning">[DRAFT]</span>{{/if}}
              {{#if status.publicCd}}    <span class="badge badge-dsuite text-bg-success">[PUBLIC CD]</span>{{/if}}
              {{#if status.active}}      <span class="badge badge-dsuite text-bg-success">[ACTIVE]</span>{{/if}}
              {{#if status.versionless}} <span class="badge badge-dsuite bg-success-subtle text-info-emphasis">[VERSIONLESS]</span>{{/if}}
              {{#if status.amended}}     <span class="badge badge-dsuite text-bg-secondary">[AMENDED]</span>{{/if}}
              {{#if status.reaffirmed}}  <span class="badge badge-dsuite text-bg-info">[REAFFIRMED]</span>{{/if}}
              {{#if status.stabilized}}  <span class="badge badge-dsuite text-bg-primary">[STABILIZED]</span>{{/if}}
              {{#if isLatestBase}}       <span class="badge badge-dsuite bg-info-subtle text-info-emphasis">[LATEST VERSION]</span>{{/if}}
              <!--{{#if __isNewestInBase}}   <span class="badge badge-dsuite text-bg-dark">NEWEST BY DATE</span>{{/if}}-->
            </div>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
    {{/if}}
  {{/if}}

      {{#if (or status/supersededBy status/amendedBy)}}
      <div class="row text-muted mx-2">
        <div class="col col-sm-6">
        {{#if status/supersededBy}}
        <br><span class="font-weight-bold small"> Superseded by:</span>
          <ul class="list-group list-group-flush">
          {{#status/supersededBy}}
              <li class="list-group-item">
                <span class="small"><a class="{{../../idType}}-link clear-filter" href="../{{this}}">{{getLabel this}}</a> [{{getStatus this}}]</span> {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li> 
          {{/status/supersededBy}}
          </ul>
        {{/if}}
        </div>
        <div class="col col-sm-6">
        {{#if status/amendedBy}}
        <br><span class="font-weight-bold small"> Amended by:</span>
          <ul class="list-group list-group-flush">
          {{#status/amendedBy}}
              <li class="list-group-item">
                <span class="small"><a class="{{../../idType}}-link clear-filter" href="../{{this}}">{{getLabel this}}</a> [{{getStatus this}}]</span> {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li> 
          {{/status/amendedBy}}
          </ul>
        {{/if}}
        </div>
      </div>
      {{/if}}

      {{#if (len xmlNamespace)}}
      <div class="card mt-3">
        <div class="card-body">
          <h2 class="h6 mb-2">XML Namespace(s) Defined</h2>
          <ul class="list-group list-group-flush small">
          {{#each (asArray xmlNamespace)}}
            <li class="list-group-item">
              <a target="_blank" href="{{this.uri}}">{{this.uri}}</a>{{#if this.supplemental}} [Supplemental]{{/if}}{{#if this.deprecated}} [Deprecated] <svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/if}}
            </li>
          {{/each}}
          </ul>
        </div>
      </div>
      {{/if}}

      {{#if (len relatedDocs)}}
      <div class="card mt-3">
        <div class="card-body">
          <h2 class="h6 mb-2">Related Document(s)</h2>
          <ul class="list-group list-group-flush small">
          {{#each (asArray relatedDocs)}}
            <li class="list-group-item">
              <a class="clear-filter" href="../{{this}}/">{{getLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
            </li>
          {{/each}}
          </ul>
        </div>
      </div>
      {{/if}}

      <!-- {{#if (len referenceTree)}}
      <div class="card mb-3">
        <div class="card-body">
          <h2 class="h6">Reference Tree</h2>
          <ul class="list-group list-group-flush small">
          {{#each (asArray referenceTree)}}
            <li class="list-group-item">
              <a href="../{{this}}/">{{getLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
            </li>
          {{/each}}
          </ul>
        </div>
      </div>
      {{/if}} -->

    </div>

    <div class="col-lg-5">
      {{#if referencesResolved}}
        {{#if (len referencesResolved.normative)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Normative Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray referencesResolved.normative)}}
              <li class="list-group-item">
                <a href="../{{id}}/" title="{{getLabel id}}">{{#if undated}}{{getUndatedLabel id}}{{else}}{{getLabel id}}{{/if}}</a>
                [{{getStatus id}}] {{{getstatusButton id "10"}}} {{#with (docProjLookup @root.docProjs id)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{else if references}}
        {{#if (len references.normative)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Normative Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray references.normative)}}
              <li class="list-group-item">
                <a href="../{{this}}/">{{getUndatedLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{/if}}

      {{#if referencesResolved}}
        {{#if (len referencesResolved.bibliographic)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Bibliographic Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray referencesResolved.bibliographic)}}
              <li class="list-group-item">
                <a href="../{{id}}/" title="{{getLabel id}}">{{#if undated}}{{getUndatedLabel id}}{{else}}{{getLabel id}}{{/if}}</a>
                [{{getStatus id}}] {{{getstatusButton id "10"}}} {{#with (docProjLookup @root.docProjs id)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{else if references}}
        {{#if (len references.bibliographic)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Bibliographic Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray references.bibliographic)}}
              <li class="list-group-item">
                <a href="../{{this}}/">{{getUndatedLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{/if}}

      {{#if (len referencedBy)}}
      <div class="card mb-3">
        <div class="card-body">
          <h2 class="h6">Referenced By</h2>
          <ul class="list-group list-group-flush small">
          {{#each (asArray referencedBy)}}
            <li class="list-group-item">
              <a href="../{{this}}/">{{getLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
            </li>
          {{/each}}
          </ul>
        </div>
      </div>
      {{/if}}
    </div>
  </section>

  <nav class="mt-3">
    <a class="btn btn-outline-secondary btn-sm" href="../#{{docId}}">← Back to Docs</a>
  </nav>
</main>

<!-- Main content ends - do not edit below this line -->
{{> footer}}