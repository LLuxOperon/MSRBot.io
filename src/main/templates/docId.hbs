{{> header}}
<!-- Main content starts - do not edit above this line -->

<main class="container my-4">
  <header class="mb-3">
    <h1 class="h4 mb-1">
      <code class="me-2">{{docLabel}}</code>
      {{#if href}}<a class="text-decoration-none" href="{{href}}" target="_blank" rel="noopener">{{docTitle}}</a>{{else}}{{docTitle}}{{/if}}
    </h1>
    
    <div class="mt-2 d-flex flex-wrap gap-1">
      {{#if status.unknown}}   <span class="label badge text-bg-danger">[ UNKNOWN ]</span>{{/if}}
      {{#if status.withdrawn}} <span class="label badge text-bg-danger">[ WITHDRAWN ]</span>{{/if}}
      {{#if status.superseded}}<span class="label badge text-bg-warning">[ SUPERSEDED ]</span>{{/if}}
      {{#if status.draft}}     <span class="label badge text-bg-warning">[ DRAFT ]</span>{{/if}}
      {{#if status.publicCd}}  <span class="label badge text-bg-success">[ PUBLIC CD ]</span>{{/if}}
      {{#if status.active}}    <span class="label badge text-bg-success">[ ACTIVE ]</span>{{/if}}
      {{#if status.versionless}}<span class="label badge text-bg-info">[ VERSIONLESS ]</span>{{/if}}
      {{#if status.amended}}   <span class="label badge text-bg-secondary">[ AMENDED ]</span>{{/if}}
      {{#if status.reaffirmed}}<span class="label badge text-bg-info">[ REAFFIRMED ]</span>{{/if}}
      {{#if status.stabilized}}<span class="label badge text-bg-primary">[ STABILIZED ]</span>{{/if}}
    </div>
  </header>

  

  <section class="row g-3">
    <div class="col-lg-7">
      <div class="card">
        <div class="card-body">
          <h2 class="h6">Details</h2>
          <dl class="row small mb-0">
            {{#if publisher}}<dt class="col-sm-4">Publisher</dt><dd class="col-sm-8">{{publisher}}</dd>{{/if}}
            {{#if docType}}<dt class="col-sm-4">Doc Type</dt><dd class="col-sm-8">{{docType}}{{#if docTypeAbr}} ({{docTypeAbr}}){{/if}}</dd>{{/if}}
            {{#if publicationDate}}<dt class="col-sm-4">Publication Date</dt><dd class="col-sm-8">{{publicationDate}}</dd>{{/if}}
            {{#if doi}}<dt class="col-sm-4">DOI</dt><dd class="col-sm-8"><a href="https://doi.org/{{doi}}" target="_blank" rel="noopener"><code>{{doi}}</code></a></dd>{{/if}}
            {{#if href}}<dt class="col-sm-4">Link</dt><dd class="col-sm-8"><a href="{{href}}" target="_blank" rel="noopener" class="text-break d-inline-block" style="max-width:100%">{{href}}</a></dd>{{/if}}
            {{#if group}}<dt class="col-sm-4">Group</dt><dd class="col-sm-8">{{#with (groupIdLookup dataGroups group)}}{{#if groupId}}<a href="/groups/{{../../htmlLink}}?search.search={{groupId}}">{{/if}}{{groupOrg}} {{groupName}} {{groupDesc}}{{#if groupId}}</a>{{/if}}{{/with}}</dd>{{/if}}
            {{#with (docProjLookup @root.docProjs docId)}}{{#if projectId}}<dt class="col-sm-4">Current Work</dt>
            <dd class="col-sm-8">
              {{#ifnoteq projectStatus "Complete"}}<span class="label badge text-bg-warning text-uppercase">{{/ifnoteq}}{{#ifeq projectStatus "Complete"}}<span class="label badge text-bg-light text-uppercase">{{/ifeq}}{{workType}} - {{projectStatus}}</span>
              <br>{{#ifnoteq projectStatus "Complete"}}<span class="small">Under Development: {{/ifnoteq}}<a href="/projects/{{../../htmlLink}}#{{projectId}}">Project - {{projectId}}</a> {{#if newDoc}} - New Doc: <a class="{{../../idType}}-link clear-filter" href="/docs/{{newDoc}}">{{getLabel newDoc}}</a> [{{getStatus newDoc}}]</span> {{{getstatusButton newDoc "10"}}} {{#with (docProjLookup @root.docProjs newDoc)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}{{/if}}</span>
            </dd>{{/if}}{{/with}}
            {{#if status.statusNote}}<dt class="col-sm-4">Status Note</dt><dd class="col-sm-8"><em>{{status.statusNote}}</em></dd>{{/if}}
          </dl>
        </div>
        
      </div>

      {{#if docBase}}
      <div class="p-2 text-muted small">
        <span class="fst-italic">{{#if status.latestVersion}}<span class="label badge text-bg-light">[ LATEST VERSION ]</span> {{else}}Newer version available{{/if}} of base {{docBaseLabel}}</span>
        {{#if status/supersededBy}}
        <br><span class="font-weight-bold small"> Superseded by:</span>
          <ul class="list-group list-group-flush">
          {{#status/supersededBy}}
              <li class="list-group-item">
                <span class="small"><a class="{{../../idType}}-link clear-filter" href="/docs/{{this}}">{{getLabel this}}</a> [{{getStatus this}}]</span> {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li> 
          {{/status/supersededBy}}
          </ul>
        {{/if}}
        
        
      </div>
      {{/if}}

      {{#if (len xmlNamespace)}}
      <div class="card mt-3">
        <div class="card-body">
          <h2 class="h6 mb-2">XML Namespace(s) Defined</h2>
          <ul class="list-group list-group-flush small">
          {{#each (asArray xmlNamespace)}}
            <li class="list-group-item">
              <a target="_blank" href="{{this.uri}}">{{this.uri}}</a>{{#if this.supplemental}} [Supplemental]{{/if}}{{#if this.deprecated}} [Deprecated] <svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/if}}
            </li>
          {{/each}}
          </ul>
        </div>
      </div>
      {{/if}}

      {{#if (len relatedDocs)}}
      <div class="card mt-3">
        <div class="card-body">
          <h2 class="h6 mb-2">Related Document(s)</h2>
          <ul class="list-group list-group-flush small">
          {{#each (asArray relatedDocs)}}
            <li class="list-group-item">
              <a class="clear-filter" href="/docs/{{this}}/">{{getLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
            </li>
          {{/each}}
          </ul>
        </div>
      </div>
      {{/if}}
    </div>

    <div class="col-lg-5">
      {{#if referencesResolved}}
        {{#if (len referencesResolved.normative)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Normative Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray referencesResolved.normative)}}
              <li class="list-group-item">
                <a href="/docs/{{id}}/" title="{{getLabel id}}">{{#if undated}}{{getUndatedLabel id}}{{else}}{{getLabel id}}{{/if}}</a>
                [{{getStatus id}}] {{{getstatusButton id "10"}}} {{#with (docProjLookup @root.docProjs id)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{else if references}}
        {{#if (len references.normative)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Normative Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray references.normative)}}
              <li class="list-group-item">
                <a href="/docs/{{this}}/">{{getUndatedLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{/if}}

      {{#if referencesResolved}}
        {{#if (len referencesResolved.bibliographic)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Bibliographic Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray referencesResolved.bibliographic)}}
              <li class="list-group-item">
                <a href="/docs/{{id}}/" title="{{getLabel id}}">{{#if undated}}{{getUndatedLabel id}}{{else}}{{getLabel id}}{{/if}}</a>
                [{{getStatus id}}] {{{getstatusButton id "10"}}} {{#with (docProjLookup @root.docProjs id)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{else if references}}
        {{#if (len references.bibliographic)}}
        <div class="card mb-3">
          <div class="card-body">
            <h2 class="h6">Bibliographic Reference(s)</h2>
            <ul class="list-group list-group-flush small">
            {{#each (asArray references.bibliographic)}}
              <li class="list-group-item">
                <a href="/docs/{{this}}/">{{getUndatedLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
              </li>
            {{/each}}
            </ul>
          </div>
        </div>
        {{/if}}
      {{/if}}

      {{#if (len referencedBy)}}
      <div class="card mb-3">
        <div class="card-body">
          <h2 class="h6">Referenced By</h2>
          <ul class="list-group list-group-flush small">
          {{#each (asArray referencedBy)}}
            <li class="list-group-item">
              <a href="/docs/{{this}}/">{{getLabel this}}</a> [{{getStatus this}}] {{{getstatusButton this "10"}}} {{#with (docProjLookup @root.docProjs this)}}{{#ifnoteq projectStatus "Complete"}}<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" fill="#F6BE00" class="bi bi-slash-circle-fill align-baseline" viewBox="0 0 16 16"><path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-4.646-2.646a.5.5 0 0 0-.708-.708l-6 6a.5.5 0 0 0 .708.708l6-6z"/></svg>{{/ifnoteq}}{{/with}}
            </li>
          {{/each}}
          </ul>
        </div>
      </div>
      {{/if}}
    </div>
  </section>

  <nav class="mt-3">
    <a class="btn btn-outline-secondary btn-sm" href="/docs/#{{docId}}">‚Üê Back to Docs</a>
  </nav>
</main>

<!-- Main content ends - do not edit below this line -->
{{> footer}}