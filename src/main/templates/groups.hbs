<!DOCTYPE html>
<html lang="en">
  {{>header}}
    <!-- Main content starts - do not edit above this line -->
    <table id="sorttableGroups" class="table table-small table-hover">
      <thead class="thead-light">
        <tr>
          <th scope="col" class="d-none"></th>
          <th scope="col">Organization</th>
          <th scope="col" class="col-2">Group</th>
          <th scope="col" class="text-center text-nowrap">Group Type</th>
          <th scope="col" class="d-none"></th>
          <th scope="col">Status</th>
          <th scope="col" class="col-3">Description</th>
          <th scope="col" class="d-none"></th>
          <th scope="col" class="col-2 text-nowrap">Parent Group</th>
          <th scope="col"></th>
          <th scope="col"></th>
          <th scope="col"></th>
          <th scope="col"></th>
          <th scope="col"></th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody id="searchTable">
        {{#data}}
        <tr id="{{../idType}}-{{groupId}}" {{#ifeq groupStatus/pending true}}class="pending"{{/ifeq}}>
          <td class="grouplabel d-none">
            <code>{{groupId}}</code>
          </td>
          <td>
            <a class="anchor" id="{{groupId}}" href="#{{groupId}}"></a>{{groupOrg}}
          </td>
          <td>
            {{#if groupLink}}<a href="{{groupLink}}" target="_blank">{{/if}}{{#if parentgroupId}}{{#with (groupIdLookup ../data parentgroupId)}}{{#ifnoteq groupType "TC"}}{{#with (groupIdLookup ../../data parentgroupId)}}{{groupName}}{{/with}} {{/ifnoteq}}{{#ifeq groupType "TC"}}{{groupName}}{{/ifeq}} - {{/with}}{{/if}}{{groupName}}{{#if groupDesc}} {{groupDesc}}{{/if}}{{#if groupLink}}</a>{{/if}}
          </td>
          <td class="text-center">
            {{groupType}}
          </td>
          <td class="d-none">
            {{#ifeq groupStatus/active true}}Active{{/ifeq}}{{#ifeq groupStatus/active false}}Closed{{/ifeq}}{{#ifeq groupStatus/pending true}}Pending{{/ifeq}}
          </td>
          <td>
            {{#ifeq groupStatus/active true}}<span class="label badge badge-success">Active</span>{{/ifeq}}{{#ifeq groupStatus/active false}}<span class="label badge badge-warning">Closed</span>{{/ifeq}}{{#ifeq groupStatus/pending true}}<span class="label badge badge-warning">Pending</span>{{/ifeq}}
          </td>
          <td>
            {{groupSummary}}
            {{#ifeq groupStatus/active false}}<span class="font-weight-bold small"><em>** NOTE: This group is no longer active{{#if groupStatus/assignedGroup}}; all work and documents (published, in draft, and repos) have been reassigned to: {{#groupStatus/assignedGroup}}{{#unless @first}}, {{/unless}}{{#with (groupIdLookup ../../data this)}}<a class="group-link clear-filter" href="#{{groupId}}">{{groupOrg}} {{#if parentgroupId}}{{#with (groupIdLookup ../../../data parentgroupId)}}{{#if parentgroupId}}{{#with (groupIdLookup ../../../../data parentgroupId)}}{{groupName}}{{/with}} {{/if}}{{groupName}}{{/with}} {{/if}}{{groupName}}{{#if groupDesc}} {{groupDesc}}{{/if}}{{#if groupLink}}{{/if}}</a>{{/with}}{{/groupStatus/assignedGroup}}{{/if}}. **</em></span>{{/ifeq}}
          </td>
          <td class="d-none">
            {{#ifeq groupType "TC"}}{{groupName}}{{#if groupDesc}} {{groupDesc}}{{/if}}{{/ifeq}}
            {{#if parentgroupId}}{{#with (groupIdLookup ../data parentgroupId)}}
            {{#ifeq groupType "TC"}}{{groupName}}{{#if groupDesc}} {{groupDesc}}{{/if}}{{/ifeq}}
            {{#ifnoteq groupType "TC"}}{{#with (groupIdLookup ../../data parentgroupId)}}{{groupName}}{{#if groupDesc}} {{groupDesc}}{{/if}}{{/with}}{{/ifnoteq}}{{/with}}{{/if}}
          </td>
          <td>
            {{#if parentgroupId}}{{#with (groupIdLookup ../data parentgroupId)}}<a class="group-link clear-filter" href="#{{groupId}}">{{groupOrg}} {{#ifnoteq groupType "TC"}}{{#with (groupIdLookup ../../data parentgroupId)}}{{groupName}}{{/with}} - {{/ifnoteq}} {{groupName}}{{#if groupDesc}} {{groupDesc}}{{/if}} {{groupType}}</a>{{/with}}{{/if}}
          </td>
          <td class="text-center">
            {{#if groupRepo}}
            <a href="{{groupRepo}}" target="_blank">Repo</a>
            {{/if}}
          </td>
          <td>
            {{#ifeq groupType "TC"}}
            <a href="../{{../htmlLink}}?search.search={{spaceReplace groupOrg}}{{#if parentgroupId}}{{#with (groupIdLookup ../dataGroups parentgroupId)}}%20{{spaceReplace groupName}}{{/with}}{{/if}}%20{{spaceReplace groupName}}{{#if groupDesc}}%20{{spaceReplace groupDesc}}{{/if}}">Published</a>
            {{/ifeq}}
          </td>
          <td>
            {{#if groupType}}
            <a href="../projects/{{../htmlLink}}?search.search={{spaceReplace groupOrg}}{{#if parentgroupId}}{{#with (groupIdLookup ../dataGroups parentgroupId)}}%20{{spaceReplace groupName}}{{/with}}{{/if}}%20{{spaceReplace groupName}}{{#if groupDesc}}%20{{spaceReplace groupDesc}}{{/if}}">Projects</a>
            {{/if}}
          </td>
          <td class="text-center">
            {{#if groupDocs}}
            <a href="{{groupDocs}}" target="_blank">Documents</a>
            {{/if}}
          </td>
          <td class="text-center">
            {{#if groupCal}}
            <a href="{{groupCal}}" target="_blank">Calendar</a>
            {{/if}}
          </td>
          <td class="text-center">
            {{#if groupBallots}}
            <a href="{{groupBallots}}" target="_blank">Ballots</a>
            {{/if}}
          </td>
          
        </tr>
        {{/data}}
      </tbody>
    </table>
    <!-- Main content ends - do not edit below this line -->
{{>footer}}

</html>