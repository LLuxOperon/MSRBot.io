<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
  <!-- SEO: core meta -->
  <meta name="description" content="{{siteDescription}}">
  <link rel="canonical" href="{{canonicalUrl}}">
  <meta name="robots" content="{{#if robotsMeta}}{{robotsMeta}}{{else}}index,follow,max-snippet:-1,max-image-preview:large,max-video-preview:-1{{/if}}">
  <meta name="author" content="{{author}} ({{copyrightHolder}})">
  <meta name="copyright" content="{{copyrightYear}} {{copyright}}">
  <meta name="license" content="{{license}}">
  <meta name="license:link" content="{{licenseUrl}}">

  <!-- Open Graph -->
  <meta property="og:type" content="website">
  <meta property="og:site_name" content="{{siteName}}">
  <meta property="og:locale" content="{{locale}}">
  <meta property="og:title" content="{{ogTitle}}">
  <meta property="og:description" content="{{ogDescription}}">
  <meta property="og:copyright" content="{{copyrightYear}} {{copyright}}">
  <meta property="og:license" content="{{license}}"> 
  <meta property="og:url" content="{{canonicalUrl}}">
  <meta property="og:image" content="{{ogImage}}">
  <meta property="og:image:secure_url" content="{{ogImage}}">
  <meta property="og:image:alt" content="{{ogImageAlt}}">
  <meta property="og:image:width" content="1200">
  <meta property="og:image:height" content="630"> 

  <!-- Twitter -->
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:image" content="{{ogImage}}">

  <!-- PWA/Theme (optional but nice) -->
  <meta name="application-name" content="{{siteName}}">
  <meta name="theme-color" content="#1398b0">

  <!-- Icons -->
  <link type="image/svg" rel="icon" href="{{assetPrefix}}static/MSRBot-PrZ3-blue.svg">
  <link rel="icon" type="image/svg+xml" href="{{assetPrefix}}static/MSRBot-PrZ3-blue.svg">
  <link rel="icon" type="image/png" sizes="32x32" href="{{assetPrefix}}static/icons/favicon-32.png">
  <link rel="icon" type="image/png" sizes="192x192" href="{{assetPrefix}}static/icons/favicon-192.png">
  <link rel="apple-touch-icon" sizes="180x180" href="{{assetPrefix}}static/icons/apple-touch-icon-180.png">
  
  <!-- Styles -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.css" rel="stylesheet">
  <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Lato&amp;subset=latin,latin-ext">
  <link rel="stylesheet" type="text/css" href="{{assetPrefix}}css/msrbot.css">

  <title>MSRBot.io | {{listTitle}}</title>

  <!-- Scripts -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

  <script src="{{assetPrefix}}js/msrbot.js"></script>

  <!-- JSON-LD: basic WebSite schema -->
  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "{{#ifeq templateName "index"}}WebSite{{/ifeq}}{{#ifnoteq templateName "index"}}WebPage{{/ifnoteq}}",
    "name": "{{#ifnoteq templateName "index"}}{{listTitle}} - {{/ifnoteq}}{{siteName}}",
    "url": "{{canonicalUrl}}",
    "inLanguage": "{{locale}}",
    "potentialAction": {
      "@type": "SearchAction",
      "target": "{{canonicalBase}}/docs/?q={query}",
      "query-input": "required name=query"
    }{{#ifnoteq templateName "index"}},
    "isPartOf": {
      "@type": "WebSite",
      "name": "{{siteName}}",
      "url": "{{canonicalBase}}"
    }{{/ifnoteq}},
    "creator": {
      "@type": "Organization",
      "name": "{{copyrightHolder}}",
      "url": "{{authorUrl}}"
    },
    "copyrightHolder": {
      "@type": "Organization",
      "name": "{{copyrightHolder}}"
    },
    "copyrightYear": "{{copyrightYear}}",
    "license": "{{licenseUrl}}"{{#ifnoteq templateName "index"}},
    "primaryImageOfPage": {
      "@type": "ImageObject",
      "url": "{{ogImage}}",
      "width": 1200,
      "height": 630
    }{{/ifnoteq}}
  }
  </script>

  <!-- Other -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link rel="search" type="application/opensearchdescription+xml" title="MSRBot" href="{{assetPrefix}}opensearch.xml">
  <link rel="license" href="https://opensource.org/licenses/BSD-3-Clause">
  
</head>
<body>
  <nav class="navbar navbar-expand-md sticky-top d-flex px-3 border-bottom border-secondary-subtle shadow">
    <div class="container">
      <div class="navbar-brand"><a class="" href="#"><img src="{{assetPrefix}}static/MSRBot-PrZ3-blue.svg" alt="MSR" width="28" height="28" class="me-2"><img src="{{assetPrefix}}static/MSRBot-Logo-blue.svg" alt="MSR" height="28"></a></div>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        
        <ul class="navbar-nav ms-auto mb-lg-0">
          <li class="nav-item text-nowrap">
            <a id="nav-home" class="nav-link d-flex flex-column text-center" aria-current="page" href="{{assetPrefix}}{{htmlLink}}"><i class="bi bi-house-fill" style="font-size: 1.3rem;"></i><span class="small">HOME</span></a>
          </li>
          <li class="nav-item text-nowrap">
            <a id="nav-docs" class="nav-link d-flex flex-column text-center" aria-current="page" href="{{assetPrefix}}docs/{{htmlLink}}"><i class="bi bi-file-richtext-fill" style="font-size: 1.3rem;"></i><span class="small">DOCS</span></a>
          </li>
          <li class="nav-item text-nowrap">
            <a id="nav-reftree" class="nav-link d-flex flex-column text-center" aria-current="page" href="{{assetPrefix}}reftree/{{htmlLink}}"><i class="bi bi-diagram-3-fill" style="font-size: 1.3rem;"></i><span class="small">REF TREE</span></a>
          </li>
          <li class="nav-item text-nowrap">
            <a id="nav-groups" class="nav-link d-flex flex-column text-center" aria-current="page" href="{{assetPrefix}}groups/{{htmlLink}}"><i class="bi bi-people-fill" style="font-size: 1.3rem;"></i><span class="small">GROUPS</span></a>
          </li>
          <li class="nav-item text-nowrap">
            <a id="nav-projects" class="nav-link d-flex flex-column text-center" aria-current="page" href="{{assetPrefix}}projects/{{htmlLink}}"><i class="bi bi-kanban-fill" style="font-size: 1.3rem;"></i><span class="small">PROJECTS</span></a>
          </li>
          <li class="nav-item text-nowrap">
            <a class="nav-link d-flex flex-column text-center" aria-current="page" href="https://github.com/PrZ3r/MSRBot.io" target="_blank" ><i class="bi bi-github" style="font-size: 1.3rem;"></i><span class="small">CONTRIBUTE</span></a>
          </li>
        </ul>

        <div class="dev-links-stack">
          <a
            id="dev-tools"
            class="dev-link"
            title="Developer data exports"
            href="#"
            role="button"
            tabindex="0"
            data-bs-toggle="popover"
            data-bs-trigger="focus"
            data-bs-placement="bottom"
          >
            Dev Tools <i class="bi bi-code-slash" width="1em" height="1em" viewBox="0 0 16 16"></i>
          </a>
          <a
            id="user-prefs"
            class="dev-link"
            title="Set Preferences"
            href="#"
            role="button"
            tabindex="0"
            data-bs-toggle="popover"
            data-bs-trigger="focus"
            data-bs-placement="bottom"
          >
            Preferences <i class="bi bi-gear" width="1em" height="1em" viewBox="0 0 16 16"></i>
          </a>
        </div>
      </div>
    </div>
  </nav>

  <!-- Hidden template for Dev Tools popover content -->
  <div id="dev-tools-popover-content" class="d-none">
    <div class="small">
      <strong>Registry JSON</strong>
      <ul class="mb-2">
        <li>
          <a href="{{assetPrefix}}docs/_data/documents.json" target="_blank" rel="noopener">
            documents.json
          </a>
          &mdash; normalized document registry
        </li>
        <li>
          <a href="{{assetPrefix}}groups/_data/groups.json" target="_blank" rel="noopener">
            groups.json
          </a>
          &mdash; normalized groups registry
        </li>
        <li>
          <a href="{{assetPrefix}}projects/_data/projects.json" target="_blank" rel="noopener">
            projects.json
          </a>
          &mdash; project registry with lookups
        </li>
      </ul>

      <strong>CSV exports</strong>
      <ul class="mb-0">
        <li>
          <a href="{{assetPrefix}}_data/documents.csv" target="_blank" rel="noopener">
            documents.csv
          </a>
          &mdash; flat document CSV export
        </li>
        <li>
          <a href="{{assetPrefix}}_data/groups.csv" target="_blank" rel="noopener">
            groups.csv
          </a>
          &mdash; flat groups CSV export
        </li>
        <li>
          <a href="{{assetPrefix}}_data/projects.csv" target="_blank" rel="noopener">
            projects.csv
          </a>
          &mdash; flat projects CSV export
        </li>
      </ul>
    </div>
  </div>

  <!-- Hidden template for Preferences popover content -->
  <div id="user-prefs-popover-content" class="d-none">
    <div class="small">
      <strong>Theme <span id="theme-current-label" class="text-muted"></span></strong>
      <p class="mb-2">Choose how MSRBot.io looks on this device.</p>
      <div class="d-grid gap-1">
        <a href="#" class="btn btn-sm btn-outline-secondary text-start" data-bs-theme-choice="system">
          Auto (match system)
        </a>
        <a href="#" class="btn btn-sm btn-outline-secondary text-start" data-bs-theme-choice="light">
          Light mode
        </a>
        <a href="#" class="btn btn-sm btn-outline-secondary text-start" data-bs-theme-choice="dark">
          Dark mode
        </a>
      </div>
      <p class="mt-2 mb-0 text-muted fst-italic">Preference is stored in this browser only.</p>
    </div>
  </div>

  <script>
    document.addEventListener('DOMContentLoaded', function () {
      // Dev Tools popover
      var devTrigger = document.getElementById('dev-tools');
      if (devTrigger && window.bootstrap && bootstrap.Popover) {
        new bootstrap.Popover(devTrigger, {
          html: true,
          container: 'body',
          sanitize: false,
          content: function () {
            var tpl = document.getElementById('dev-tools-popover-content');
            return tpl ? tpl.innerHTML : '';
          }
        });
      }

      // Preferences popover (theme selection)
      var prefsTrigger = document.getElementById('user-prefs');
      if (prefsTrigger && window.bootstrap && bootstrap.Popover) {
        new bootstrap.Popover(prefsTrigger, {
          html: true,
          container: 'body',
          sanitize: false,
          content: function () {
            var tpl = document.getElementById('user-prefs-popover-content');
            return tpl ? tpl.innerHTML : '';
          }
        });
      }
    });
  </script>

  <a href="#" id="toTopBtn" class="cd-top text-replace js-cd-top cd-top--is-visible cd-top--fade-out" data-abc="true"></a>
  <a class="anchor" id="top" href="#top"></a>